{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "ecmaFeatures": { "jsx": true }
  },
  "rules": {
    "react/prop-types": "off",
    "no-unused-vars": "off",
    "no-useless-catch": "off",
    "no-console": "off"
  },
  "settings": {
    "react": { "version": "detect" }
  }
}
